<!DOCTYPE html>
<html>
    <head>
        <title>Home</title>
        <link href="/Style.css" rel="stylesheet" type="text/css" />

    </head>
    <body>
        <%- include("./partials/nav.ejs") %>
        <h1>Blog's</h1>
        <div class="container">
            <% data.forEach(title=>{ %>
                <div class="row-blog">
                
                    <a href="blog/main/<%= title.substr(0, (title.length-4)) %>"><h4><%= title.substr(0, (title.length-4)) %></h4></a>
                 </div>
                 <% }) %>
        </div>

    </body>
</html>
